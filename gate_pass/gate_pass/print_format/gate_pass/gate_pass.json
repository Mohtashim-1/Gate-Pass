 "html": "<style type=\"text/css\">\r\n.address {\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:13px;\r\n}\r\n.address-heading {\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:15px;\r\n\tfont-weight:bold;\r\n}\r\n.address-heading-center {\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:15px;\r\n\tfont-weight:bold;\r\n\ttext-align:center;\r\n}\r\n.address-heading-center-white {\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:15px;\r\n\tfont-weight:bold;\r\n\ttext-align:center;\r\n\tcolor:#FFF;\r\n}\r\n.address-heading-left {\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:15px;\r\n\tfont-weight:bold;\r\n\ttext-align:left;\r\n}\r\n.address-heading-left-white {\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:15px;\r\n\tfont-weight:bold;\r\n\ttext-align:left;\r\n\tcolor:#FFF;\r\n}\r\n.heading {\tfont-family: \"Arial Black\", Gadget, sans-serif;\r\n\tfont-size: 20px;\r\n\tfont-weight: bold;\r\n\ttext-align:center;\r\n\tcolor:076ba0;\r\n}\r\n.heading-right {\tfont-family: \"Arial Black\", Gadget, sans-serif;\r\n\tfont-size: 25px;\r\n\tfont-weight: bold;\r\n\ttext-align:right;\r\n\tcolor:076ba0;\r\n}\r\n.normal-content {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:14px;\r\n\ttext-align:left;\r\n}\r\n.normal-content-center {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:14px;\r\n\ttext-align:center;\r\n}\r\n\r\n.normal-content-bold-center {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:14px;\r\n\ttext-align:center;\r\n\tfont-weight:bold;\r\n}\r\n.normal-content-bold-left {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:14px;\r\n\ttext-align:left;\r\n\tfont-weight:bold;\r\n}\r\n.normal-content-bold-right {\t\tfont-family:Arial, Helvetica, sans-serif;\r\n\tfont-size:14px;\r\n\ttext-align:right;\r\n\tfont-weight:bold;\r\n}\r\n#border-bottom {\t\r\nborder-bottom:solid 3px;\r\nborder-bottom-color:#076ba0;\r\n\r\n\r\n}\r\n#border-bg {\t\r\n\r\nbackground-color:#076ba0;\r\n\r\n\r\n}\r\n#border-bottom-light {\t\r\nborder-bottom:solid 2px;\r\nborder-bottom-color:#076ba0;\r\n\r\n\r\n}\r\n#border-bottom-grey {\t\r\nborder-bottom:solid 2px;\r\nborder-bottom-color:#CCC;\r\n\r\n\r\n}\r\n#border-bottom-right {\t\r\nborder-right:solid 2px;\r\nborder-right-color:#076ba0;\r\n\r\n\r\n}\r\n</style>\r\n{%if doc.workflow_state == \"Draft\"%}\r\n<p class=\"heading\">Please Submit the Document To Get the Print!</p>\r\n{%else%}\r\n<table width=\"100%\">\r\n  <tr>\r\n    <td id=\"border-bottom\"><img src=\"https://erp.sahexports.com/files/SAH_logo.png\" width=\"200px\"><br>\r\n      <span class=\"address-heading\">(HOME TEXTILE DIVISION)</span><br><span class=\"address\">Plot # :4,5,6,13,14,15, Sector B-IX, <br>\r\nKarachi Export Processing Zone,<br>\r\nKarachi 75150 Pakistan.<br>\r\nPh 021-35080157<br>\r\nWebsite : www.sahent.com</span></td>\r\n    <td width=\"40%\" class=\"heading-right\" id=\"border-bottom\">{%if doc.type ==\"Returnable\"%}RETURNABLE GATE PASS{%endif%}{%if doc.type ==\"Non-Returnable\"%}GATE PASS{%endif%}</td>\r\n  </tr>\r\n\r\n</table>\r\n\r\n<table width=\"100%\">\r\n  <tr>\r\n    <td width=\"30%\" id=\"border-bottom-light\"><span class=\"normal-content-bold-left\">Send To :<br>{%- if doc.customer -%}{{doc.customer}}{%-endif-%}{%- if doc.supplier -%}{{doc.supplier}}{%-endif-%}{%- if doc.local -%}{{doc.local}}{%-endif-%}</span></td>\r\n    <td rowspan=\"2\" width=\"37%\" id=\"border-bottom-light\">&nbsp;</td>\r\n    <td rowspan=\"2\" class=\"normal-content-bold-left\" width=\"13%\" id=\"border-bottom-light\">GATE PASS: <br />\r\n\t\t\t\t\t\t\t\tDATE:<br>\r\n\t\t\t\t\t\t\t\tBy Hand:\r\n    <br></td>\r\n    <td rowspan=\"2\" class=\"normal-content\" width=\"20%\" id=\"border-bottom-light\">{{doc.name}}<br>\t    {{doc.get_formatted('creation_date')}}<br>\r\n     {{doc.by_hand}}</td>\r\n  </tr>\r\n\r\n</table>\r\n\r\n{%if doc.inventory_gate_pass%}\r\n<table width=\"100%\">\r\n  <tr>\r\n    <td width=\"5%\" rowspan=\"2\" class=\"address-heading-center-white\" id=\"border-bg\">S.No </td>\r\n    <td width=\"83%\" rowspan=\"2\" class=\"address-heading-left-white\" id=\"border-bg\">Item Description</td>\r\n    <td width=\"12%\" class=\"address-heading-center-white\" id=\"border-bg\">QTY</td>\r\n  </tr>\r\n  <tr>\r\n    \r\n  </tr>\r\n  {%- for cor in doc.inventory_gate_pass-%}\r\n  <tr>\r\n    <td class=\"normal-content-center\" id=\"border-bottom-grey\"><span class=\"normal-content\">{{cor.idx}}</span></td>\r\n    <td class=\"normal-content-center\" id=\"border-bottom-grey\"><div align=\"left\">{{cor.item}}</div></td>\r\n    <td class=\"normal-content-center\" id=\"border-bottom-grey\">{{cor.qty}} {{cor.uom}}</td>\r\n  </tr>\r\n  {%endfor%}\r\n\r\n</table>\r\n{%endif%}\r\n{%if doc.non_inventory%}\r\n<table width=\"100%\">\r\n  <tr>\r\n    <td width=\"5%\" rowspan=\"2\" class=\"address-heading-center-white\" id=\"border-bg\">S.No </td>\r\n    <td width=\"83%\" rowspan=\"2\" class=\"address-heading-left-white\" id=\"border-bg\">Item Description</td>\r\n    <td width=\"12%\" class=\"address-heading-center-white\" id=\"border-bg\">QTY</td>\r\n  </tr>\r\n  <tr>\r\n    \r\n  </tr>\r\n  {%- for cor in doc.non_inventory-%}\r\n  <tr>\r\n    <td class=\"normal-content-center\" id=\"border-bottom-grey\"><span class=\"normal-content\">{{cor.idx}}</span></td>\r\n    <td class=\"normal-content-center\" id=\"border-bottom-grey\"><div align=\"left\">{{cor.item}}</div></td>\r\n    <td class=\"normal-content-center\" id=\"border-bottom-grey\">{{cor.qty}} {{cor.uom}}</td>\r\n  </tr>\r\n  {%endfor%}\r\n\r\n</table>\r\n{%endif%}\r\n\r\n<table width=\"100%\" align=\"left\">\r\n  <tr>\r\n    <td class=\"normal-content-bold-left\" width=\"53%\"> </td>\r\n  </tr>\r\n  <tr>\r\n    <td width=\"53%\"><span class=\"normal-content-bold-left\">Remarks :</span><span class=\"normal-content\"> {{doc.additional_instruction or ''}}</span></td>\r\n  </tr>\r\n</table>\r\n<br>\r\n<br>\r\n<br>\r\n<br>\r\n<br>\r\n<br>\r\n<br>\r\n<br>\r\n<table width=\"100%\" align=\"center\">\r\n  <tr>\r\n    <td id=\"border-bottom\" class=\"normal-content-bold-center\" width=\"20%\">{{doc.prepared_by}}<br>    </td>\r\n    <td width=\"20%\">&nbsp;</td>\r\n    <td id=\"border-bottom\" class=\"normal-content-bold-center\" width=\"20%\">{{doc.approved_user or 'Not Approved'}}</td>\r\n    <td width=\"20%\"></td>\r\n    <td width=\"20%\" id=\"border-bottom\" class=\"normal-content-bold-center\">&nbsp;</td>\r\n  </tr>\r\n  <tr>\r\n    <td class=\"normal-content-bold-center\">PREPARED BY<br>\r\n      {{doc.get_formatted('creation')}}</td>\r\n    <td>&nbsp;</td>\r\n    <td class=\"normal-content-bold-center\">APPROVED BY<br>      \r\n     {%if doc.approved_user%} {{doc.get_formatted('modified')}}{%endif%}</td>\r\n    <td class=\"normal-content-bold-center\">&nbsp;</td>\r\n    <td class=\"normal-content-bold-center\">RECEIVED BY</td>\r\n  </tr>\r\n</table>\r\n{%endif%}",
